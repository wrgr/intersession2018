    "    for x in range(len(Xval)):\n",
    "        neighbors = getNeighbors(Xtr, Xval[x], k)\n",
    "        result = getResponse(neighbors)\n",
    "        predictions.append(result)\n",
    "    print(predictions)\n",
    "    print(Yval)\n",
    "    print('accuracy: %f' % np.mean(predictions == Yval))"
   ]
  }
